@using R
<fieldset>
    <legend>
        @Resource.Vegetariandiet
    </legend>
    

    <div>
        <h1>
            @Resource.Vegetariandiet
        </h1>
        <img src="~/Content/Images/dit5.jpg" width="550px" height="550px " />
        <h1> @Resource.toEat</h1>
        <ul>
            <li>@Resource.Fruits7 @*Fruits: Apples, bananas, berries, oranges, melons, pears, peaches*@</li>
            <li>@Resource.Vegetables7 @*Vegetables: Leafy greens, asparagus, broccoli, tomatoes, carrots*@</li>
            <li>@Resource.Grains7 @*Grains: Quinoa, barley, buckwheat, rice, oats*@</li>
            <li>@Resource.Legumes7 @*Legumes: Lentils, beans, peas, chickpeas*@.</li>
            <li>@Resource.Nuts7 @*Nuts: Almonds, walnuts, cashews, chestnuts*@</li>
            <li>@Resource.Seeds7 @*Seeds: Flaxseeds, chia and hemp seeds*@</li>
            <li>@Resource.Healthyfats7 @*Healthy fats: Coconut oil, olive oil, avocados*@</li>
            <li>@Resource.Proteins7 @*Proteins: Tempeh, tofu, seitan, natto, nutritional yeast, spirulina, eggs, dairy products*@</li>
        </ul>
        <h1>@Resource.toAvoid</h1>
        
        <ul>
            <li>@Resource.Meat7 @*Meat: Beef, veal and pork*@</li>
            <li>@Resource.Poultry7 @*Poultry: Chicken and turkey*@</li>
            <li>@Resource.Fishandshellfish7 @*Fish and shellfish: This restriction does not apply to pescetarians*@.</li>
            <li>@Resource.Meatbasedingredients7 @*Meat-based ingredients: Gelatin, lard, carmine, oleic acid and suet*@</li>
            <li>@Resource.Eggs7 @*Eggs: This restriction applies to vegans and lacto-vegetarians*@.</li>
            <li>@Resource.Dairyproducts7 @*Dairy products: This restriction on milk, yogurt and cheese applies to vegans and ovo-vegetarians*@.</li>
            <li>@Resource.Otheranimalproducts7 @*Other animal products: Vegans may choose to avoid honey, beeswax and pollen.*@</li>
        </ul>
        <h1>@Resource.menu</h1>
     
        <h2> @Resource.DayOne </h2>
        <ul>
            <li><label>@Resource.Breakfast</label>:@Resource.Oatmeal7 @*Oatmeal with fruit and flaxseeds*@</li>
            <li><label>@Resource.Lunch</label>:@Resource.Grilledveggie7 @*Grilled veggie and hummus wrap with sweet potato fries*@</li>
            <li><label>@Resource.Dinner</label>:@Resource.Tofu7 @*Tofu banh mi sandwich with pickled slaw*@</li>
        </ul>
        <h2>@Resource.DayTwo  </h2>
        <ul>
            <li><label>@Resource.Breakfast</label>:@Resource.Scrambled7 @*Scrambled eggs with tomatoes, garlic and mushrooms*@</li>
            <li><label>@Resource.Lunch</label>:@Resource.Zucchini7 @*Zucchini boats stuffed with veggies and feta with tomato soup*@</li>
            <li><label>@Resource.Dinner</label>:@Resource.Chickpea7 @*Chickpea curry with basmati rice*@</li>
        </ul>
        <h2> @Resource.DayThree</h2>
        <ul>
            <li><label>@Resource.Breakfast</label>:@Resource.Greekyogurt7 @*Greek yogurt with chia seeds and berries*@</li>
            <li><label>@Resource.Lunch</label>:@Resource.Farro7 @*Farro salad with tomatoes, cucumber and feta with spiced lentil soup*@</li>
            <li><label>@Resource.Dinner</label>:@Resource.Eggplant7 @*Eggplant parmesan with a side salad*@</li>
        </ul>
        <h2> @Resource.DayFour</h2>
        <ul>
            <li><label>@Resource.Breakfast</label>@Resource.Tofuu7@*: Tofu scramble with sauteed peppers, onions and spinach*@</li>
            <li><label>@Resource.Lunch</label>@Resource.Burrito7@*: Burrito bowl with brown rice, beans, avocado, salsa and veggies*@</li>
            <li><label>@Resource.Dinner</label>@Resource.Vegetables7@*: Vegetable paella with a side salad*@</li>
        </ul>
        <h2> @Resource.DayFive </h2>
        <ul>
            <li><label>@Resource.Breakfast</label>@Resource.Whole7@*: Whole-wheat toast with avocado and nutritional yeast*@</li>
            <li><label>@Resource.Lunch</label>@*: Marinated tofu pita pocket with Greek salad*@</li>
            <li><label>@Resource.Dinner</label>@Resource.Quinoablack7@*: Quinoa-black-bean meatballs with zucchini noodles*@</li>
        </ul>
        <h2> @Resource.DaySix</h2>
        <ul>
            <li><label>@Resource.Breakfast</label>@Resource.Smoothie7@*: Smoothie of kale, berries, bananas, nut butter and almond milk*@</li>
            <li><label>@Resource.Lunch</label>@Resource.Red7@*: Red lentil veggie burger with avocado salad*@</li>
            <li><label>@Resource.Dinner</label>@Resource.Flatbread7@*: Flatbread with grilled garden vegetables and pesto*@</li>
        </ul>
        <h2>@Resource.DaySeven</h2>
        <ul>

            <li><label>@Resource.Breakfast</label>:@Resource.Greekyogurt7 @*Greek yogurt with chia seeds and berries*@</li>
            <li><label>@Resource.Lunch</label>:@Resource.Farro7 @*Farro salad with tomatoes, cucumber and feta with spiced lentil soup*@</li>
            <li><label>@Resource.Dinner</label>:@Resource.Eggplant7 @*Eggplant parmesan with a side salad*@</li>
        </ul>
    </div>
